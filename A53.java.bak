import java.util.Scanner;
class A53 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.println("Enter 2 vales after press enter");
		int no1 = s.nextInt();
		int no2 = s.nextInt();
		if (no1<0)
		{no1 = no1*-1;
		}
